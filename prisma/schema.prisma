generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model ipt {
  an              String   @id
  vn              String
  hn              String
  regdate         DateTime @db.Date
  regtime         DateTime @db.Time(3)
  bw              Float
  lockdx          Int
  an_lock         String
  incharge_doctor String
}
